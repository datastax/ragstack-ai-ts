{
  "preset": "ts-jest",
  "testTimeout": 180000,
  "setupFiles": ["dotenv/config"],
  "detectOpenHandles": true,
  "transform": {
    "^.+\\.ts?$": [
      "ts-jest",
      {
        "useESM": true
      }
    ]
  },
  "extensionsToTreatAsEsm": [".ts"],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  }
}
