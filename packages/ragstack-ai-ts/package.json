{
  "name": "@datastax/ragstack-ai-ts",
  "description": "DataStax RAGStack TS",
  "version": "0.1.3",
  "license": "BUSL-1.1",
  "author": {
    "name": "DataStax"
  },
  "keywords": [
    "llm",
    "ai",
    "rag",
    "chain",
    "embeddings",
    "vectorstores",
    "astradb"
  ],
  "engines": {
    "node": ">=18"
  },
  "main": "dist/index.js",
  "dependencies": {
    "@datastax/astra-db-ts": "0.1.4",
    "@langchain/azure-openai": "0.0.2",
    "@langchain/community": "0.0.33",
    "@langchain/core": "0.1.36",
    "@langchain/openai": "0.0.26",
    "cassandra-driver": "4.7.2",
    "langchain": "0.1.23",
    "langsmith": "0.1.8"
  },
  "devDependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/client-bedrock-runtime": "^3.525.0",
    "@aws-sdk/client-s3": "^3.525.0",
    "@aws-sdk/credential-provider-node": "^3.525.0",
    "@aws-sdk/types": "^3.523.0",
    "@azure/storage-blob": "^12.17.0",
    "@langchain/google-genai": "*",
    "@smithy/eventstream-codec": "^2.1.3",
    "@smithy/protocol-http": "^3.2.1",
    "@smithy/signature-v4": "^2.1.3",
    "@smithy/util-utf8": "^2.1.1",
    "d3-dsv": "^3.0.1",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.6.3",
    "testcontainers": "^10.7.1"
  },
  "scripts": {
    "set-lib-version": "node -p \"'/*File auto-generated by package.json script*/\\nexport const RAGSTACK_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts",
    "postinstall": "yarn run set-lib-version",
    "build": "tsc --b",
    "test:unit": "yarn run -T jest tests/unit",
    "test:integration": "yarn run -T jest tests/integration",
    "test": "yarn run test:unit && yarn run test:integration",
    "release:set-version": "yarn -v && yarn version $NEW_VERSION && yarn run set-lib-version",
    "release:publish": "yarn -v && yarn run build && yarn npm publish --access public"
  }
}
